#include <iostream>

int main() {
    // Получаем от пользователя объем (V) и массу (m) тела
    double V, m;
    std::cout << "Введите объем тела (в кубических метрах): ";
    std::cin >> V;
    std::cout << "Введите массу тела (в килограммах): ";
    std::cin >> m;

    // Вычисляем плотность (ρ): Плотность = Масса / Объем
    double плотность = m / V;

    // Выводим результат на экран
    std::cout << "Плотность материала тела: " << плотность << " килограмм/кубический метр\n";

    return 0;
}
